<?xml version="1.0"?>
<Container version="2">
  <Name>Parse Server</Name>
  <Repository>bitnami/parse</Repository>
  <Registry>https://hub.docker.com/r/bitnami/parse</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/bitnami/parse</Support>
  <Project/>
  <Overview>
  Bitnami Docker Image for Parse! 
  </Overview>
  <Category>Productivity:</Category>
  <WebUI>http://[IP]:[PORT:1337]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/steinm6/docker-templates/main/steinm/icons/parse.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1619446849</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>
  Parse Server is an open source version of the Parse backend that can be deployed to any infrastructure that can run Node.js.
  
  Requires MongoDB.
  </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>1337</HostPort>
        <ContainerPort>1337</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/bitnami-parse</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
	<Variable>
	  <Name>PARSE_HOST</Name>
      <Value>127.0.0.1</Value>
      <Mode/>
    </Variable>
	<Variable>
	  <Name>PARSE_PORT_NUMBER</Name>
      <Value>1337</Value>
      <Mode/>
    </Variable>
	<Variable>
	  <Name>PARSE_MOUNT_PATH</Name>
      <Value>/parse</Value>
      <Mode/>
    </Variable>
	<Variable>
	  <Name>PARSE_APP_ID</Name>
      <Value>myappID</Value>
      <Mode/>
    </Variable>
	<Variable>
	  <Name>PARSE_MASTER_KEY</Name>
      <Value>mymasterKey</Value>
      <Mode/>
    </Variable>
	<Variable>
	  <Name>PARSE_ENABLE_CLOUD_CODE</Name>
      <Value>no</Value>
      <Mode/>
    </Variable>  
    <Variable>
	  <Name>MONGODB_HOST</Name>
      <Value>mongodb</Value>
      <Mode/>
    </Variable>
	<Variable>
      <Value>27017</Value>
      <Name>MONGODB_PORT_NUMBER</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Host Port" Target="1337" Default="" Mode="tcp" Description="Container Port: 1337" Type="Port" Display="always" Required="true" Mask="false">1337</Config>
</Container>
