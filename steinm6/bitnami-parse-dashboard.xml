<?xml version="1.0"?>
<Container version="2">
  <Name>Bitnami Parse Dashboard</Name>
  <Repository>bitnami/parse-dashboard</Repository>
  <Registry>https://hub.docker.com/r/bitnami/parse-dashboard</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/bitnami/parse-dashboard</Support>
  <Project/>
  <Overview>
  Parse Dashboard is a standalone dashboard for managing your Parse apps. You can use it to manage your Parse Server apps and your apps that are running on Parse.com.
  </Overview>
  <Category>Productivity:</Category>
  <WebUI>http://[IP]:[PORT:4040]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/steinm6/docker-templates/main/steinm6/bitnami-parse-dashboard.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/steinm6/docker-templates/main/steinm6/icons/parse.png</Icon>
  <DateInstalled>1619446849</DateInstalled>
  <Description>
  Parse Dashboard is a standalone dashboard for managing your Parse apps. You can use it to manage your Parse Server apps and your apps that are running on Parse.com.
  </Description>
  <Network>bridge</Network>
  <Environment/>
  <Labels/>
  <Config Name="Host Port" Target="4040" Default="4040" Mode="tcp" Description="Host Port" Type="Port" Display="always" Required="true" Mask="false">4040</Config>
  
  <Config Name="PARSE_DASHBOARD_USER" Target="PARSE_DASHBOARD_USER" Default="user" Mode="" Description="Parse Dashboard application username." Type="Variable" Display="always" Required="false" Mask="false"></Config> 
  
  <Config Name="PARSE_DASHBOARD_PASSWORD" Target="PARSE_DASHBOARD_PASSWORD" Default="bitnami" Mode="" Description="Parse Dashboard application password." Type="Variable" Display="always" Required="false" Mask="false"></Config> 
   
  <Config Name="PARSE_HOST" Target="PARSE_HOST" Default="user" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config> 
   
  <Config Name="PARSE_PROTOCOL" Target="PARSE_PROTOCOL" Default="http" Mode="" Description="Protocol used by Parse Dashboard to form the URLs to Parse Server." Type="Variable" Display="always" Required="false" Mask="false"></Config> 
   
  <Config Name="PARSE_USE_HOSTNAME" Target="PARSE_USE_HOSTNAME" Default="no" Mode="" Description="Whether to use a hostname or an IP to configure the serverURL setting." Type="Variable" Display="always" Required="false" Mask="false"></Config> 
   
  <Config Name="PARSE_PORT_NUMBER" Target="PARSE_PORT_NUMBER" Default="1337" Mode="" Description="Parse Server Port." Type="Variable" Display="always" Required="false" Mask="false"></Config> 
   
  <Config Name="PARSE_APP_ID" Target="PARSE_APP_ID" Default="myappID" Mode="" Description="Parse Server App Id." Type="Variable" Display="always" Required="false" Mask="false"></Config> 
   
  <Config Name="PARSE_MASTER_KEY" Target="PARSE_MASTER_KEY" Default="mymasterKey" Mode="" Description="Parse Server Master Key." Type="Variable" Display="always" Required="false" Mask="false"></Config> 
   
  <Config Name="PARSE_DASHBOARD_APP_NAME" Target="PARSE_DASHBOARD_APP_NAME" Default="MyDashboard" Mode="" Description="Parse Dashboard application name." Type="Variable" Display="always" Required="false" Mask="false"></Config> 
   
</Container>
