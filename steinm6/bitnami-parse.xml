<?xml version="1.0"?>
<Container version="2">
  <Name>Bitnami Parse Server</Name>
  <Repository>bitnami/parse</Repository>
  <Registry>https://hub.docker.com/r/bitnami/parse</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/bitnami/parse</Support>
  <Project/>
  <Overview>
  Bitnami Docker Image for Parse! 
  </Overview>
  <Category>Productivity:</Category>
  <WebUI>http://[IP]:[PORT:1337]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/steinm6/docker-templates/main/steinm6/bitnami-parse.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/steinm6/docker-templates/main/steinm6/icons/parse.png</Icon>
  <DateInstalled>1619446849</DateInstalled>
  <Description>
  Parse Server is an open source version of the Parse backend that can be deployed to any infrastructure that can run Node.js.
  
  Requires MongoDB.
  </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>1337</HostPort>
        <ContainerPort>1337</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/bitnami-parse</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Host Port" Target="1337" Default="1337" Mode="tcp" Description="Host Port" Type="Port" Display="always" Required="true" Mask="false">1337</Config>
  
  <Config Name="/parse" Target="/parse" Default="/mnt/user/appdata/bitnami-parse" Mode="rw" Description="Container Path: /bitnami&#13;&#10;Should be in appdata" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/bitnami-parse</Config>
  
  <Config Name="PARSE_HOST" Target="PARSE_HOST" Default="127.0.0.1" Mode="" Description="Parse server host" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  
  <Config Name="PARSE_PORT_NUMBER" Target="PARSE_PORT_NUMBER" Default="1337" Mode="" Description="Parse server port" Type="Variable" Display="always" Required="true" Mask="false">1337</Config>
  
  <Config Name="PARSE_MOUNT_PATH" Target="PARSE_MOUNT_PATH" Default="/parse" Mode="" Description="Parse server mount path" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  
  <Config Name="PARSE_APP_ID" Target="PARSE_APP_ID" Default="myappID" Mode="" Description="Parse server App ID" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  
  <Config Name="PARSE_MASTER_KEY" Target="PARSE_MASTER_KEY" Default="mymasterKey" Mode="" Description="Parse server Master Key" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  
  <Config Name="PARSE_ENABLE_CLOUD_CODE" Target="PARSE_ENABLE_CLOUD_CODE" Default="no" Mode="" Description="Enable Parse Cloud Code support: yes or no" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  
  <Config Name="MONGODB_HOST" Target="MONGODB_HOST" Default="mongodb" Mode="" Description="Hostname for Mongodb server" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  
  <Config Name="MONGODB_PORT_NUMBER" Target="MONGODB_PORT_NUMBER" Default="27017" Mode="" Description="Port used by Mongodb server" Type="Variable" Display="always" Required="true" Mask="false"></Config> 
  
  <Config Name="MONGODB_DATABASE_NAME" Target="MONGODB_DATABASE_NAME" Default="parse" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"></Config> 
  
  <Config Name="MONGODB_DATABASE_USER" Target="MONGODB_DATABASE_USER" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"></Config> 
  
  <Config Name="MONGODB_DATABASE_PASSWORD" Target="MONGODB_DATABASE_PASSWORD" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"></Config> 
  
  <Config Name="ALLOW_EMPTY_PASSWORD" Target="ALLOW_EMPTY_PASSWORD" Default="no" Mode="" Description="yes to allow a blank password. This is only recommended for development environments." Type="Variable" Display="always" Required="false" Mask="false"></Config> 
   
   

  
  
  
  
</Container>
